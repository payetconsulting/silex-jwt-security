{
    "name": "payetconsulting/silex-jwt-security",
    "description": "(FORK) JSON web token security provider for Silex 2.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PayetConsulting\\JWT\\": "src"
        },
        "exclude-from-classmap": [
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PayetConsulting\\JWT\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "silex/silex": "^2.1",
        "lcobucci/jwt": "^3.2",
        "symfony/security": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
